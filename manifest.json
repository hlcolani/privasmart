{
  "name": "Privasmart",
  "version": "1.0",
  "description": "Demystifying Online Privacy Policies",
  "permissions": ["activeTab", "declarativeContent", "storage", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    "popup.html"
  ],
  "manifest_version": 2
}
